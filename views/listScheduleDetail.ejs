<div id="main_container" >
<div class="filter">
<div class="global-wrap">
    <%- include('sectionDesktop')%>

<div id="app" class="subpage">
    <div class="panel-dark">
        <div class="container">
            <div>
                <div class="gap gap-section"></div>
                <div class="gap gap-mini"></div>
                <div class="bs-callout bs-callout-info"></div>

                <!-- payment in cash office -->

                <!-- Disclaimer Safe Sender List -->
